   "source": [
    "n = int(input(\"Please enter a limit number: \"))\n",
    "prime_list = []\n",
    "for i in range(1, n+1):\n",
    "    counter = 0\n",
    "    for j in range(1, i+1):\n",
    "        if i % j == 0:\n",
    "            counter += 1    \n",
    "    if counter == 2:\n",
    "        prime_list.append(i)\n",
    "print(prime_list)"
   ]
  }
